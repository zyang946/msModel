            <layout xsi:type="properties:GridLayoutDescription" numberOfColumns="4"/>
          </controls>
          <controls xsi:type="properties:ContainerDescription">
            <controls xsi:type="properties:ButtonDescription" buttonLabelExpression="Add Primitive Variable">
              <initialOperation>
                <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:self">
                  <subModelOperations xsi:type="tool_1:CreateInstance" typeName="rEMODEL.Attribute" referenceName="temp_property" variableName="newAttribute">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="aql:'attribute'+self.eContainerOrSelf(rEMODEL::Service).temp_property->filter(rEMODEL::Attribute)->size()"/>
